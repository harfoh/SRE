    "assert": "^2.1.0",
    "async-hooks": "^1.3.1",
    "babel-loader": "^8.2.5", 
    "browserify-zlib": "^0.2.0",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "net-browserify": "^0.2.4",
    "os-browserify": "^0.3.0",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
